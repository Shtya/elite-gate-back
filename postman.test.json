  "item": [
    {
      "name": "Traffic - Partners",
      "item": [
        {
          "name": "Create Partner",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Test Partner\",\n  \"kind\": \"external\",\n  \"platform\": \"facebook\",\n  \"campaignId\": 1,\n  \"baseShareUrl\": \"https://frontend.example.com/landing\",\n  \"utm\": {\n    \"utm_source\": \"facebook\",\n    \"utm_campaign\": \"summer_campaign\",\n    \"utm_content\": \"banner_1\"\n  },\n  \"email\": \"partner@example.com\",\n  \"passwordHash\": \"PlainPassword123!\",\n  \"visitRewardAmount\": 5,\n  \"registrationRewardAmount\": 20,\n  \"appointmentRewardAmount\": 50,\n  \"purchaseSharePercent\": 2.5\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{url}}/traffic/partners",
              "host": [
                "{{url}}"
              ],
              "path": [
                "traffic",
                "partners"
              ]
            }
          },
          "response": []
        },
        {
          "name": "Build Share URL for Partner",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"baseShareUrl\": \"https://frontend.example.com/landing\",\n  \"utm\": {\n    \"utm_source\": \"facebook\",\n    \"utm_campaign\": \"summer_campaign\",\n    \"utm_content\": \"banner_1\"\n  }\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{url}}/traffic/partners/:id/share-url",
              "host": [
                "{{url}}"
              ],
              "path": [
                "traffic",
                "partners",
                ":id",
                "share-url"
              ]
            },
            "description": "Replace :id with partner id"
          },
          "response": []
        },
        {
          "name": "Get Partner Performance",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{url}}/traffic/partners/:id/performance?startDate={{startDate}}&endDate={{endDate}}",
              "host": [
                "{{url}}"
              ],
              "path": [
                "traffic",
                "partners",
                ":id",
                "performance"
              ],
              "query": [
                {
                  "key": "startDate",
                  "value": "{{startDate}}"
                },
                {
                  "key": "endDate",
                  "value": "{{endDate}}"
                }
              ]
            },
            "description": "Replace :id with partner id. startDate/endDate should be ISO dates."
          },
          "response": []
        },
        {
          "name": "List Partners",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{url}}/traffic/partners?page={{page}}&limit={{limit}}&q={{q}}&isActive={{isActive}}&campaignId={{campaignId}}",
              "host": [
                "{{url}}"
              ],
              "path": [
                "traffic",
                "partners"
              ],
              "query": [
                {
                  "key": "page",
                  "value": "{{page}}"
                },
                {
                  "key": "limit",
                  "value": "{{limit}}"
                },
                {
                  "key": "q",
                  "value": "{{q}}"
                },
                {
                  "key": "isActive",
                  "value": "{{isActive}}"
                },
                {
                  "key": "campaignId",
                  "value": "{{campaignId}}"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Partner By Id",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{url}}/traffic/partners/:id",
              "host": [
                "{{url}}"
              ],
              "path": [
                "traffic",
                "partners",
                ":id"
              ]
            },
            "description": "Replace :id (note: service.ensurePartner uses user.id for lookup)."
          },
          "response": []
        },
        {
          "name": "Update Partner",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Updated Partner Name\",\n  \"kind\": \"internal\",\n  \"platform\": \"instagram\",\n  \"campaignId\": 1\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{url}}/traffic/partners/:id",
              "host": [
                "{{url}}"
              ],
              "path": [
                "traffic",
                "partners",
                ":id"
              ]
            }
          },
          "response": []
        },
        {
          "name": "Delete Partner",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{url}}/traffic/partners/:id",
              "host": [
                "{{url}}"
              ],
              "path": [
                "traffic",
                "partners",
                ":id"
              ]
            }
          },
          "response": []
        },
        {
          "name": "Adjust Partner Balance",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"amount\": 100\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{url}}/traffic/partners/:id/balance",
              "host": [
                "{{url}}"
              ],
              "path": [
                "traffic",
                "partners",
                ":id",
                "balance"
              ]
            }
          },
          "response": []
        },
        {
          "name": "Withdraw For Partner",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"amount\": 50\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{url}}/traffic/partners/:id/withdraw",
              "host": [
                "{{url}}"
              ],
              "path": [
                "traffic",
                "partners",
                ":id",
                "withdraw"
              ]
            }
          },
          "response": []
        }
      ]
    },
